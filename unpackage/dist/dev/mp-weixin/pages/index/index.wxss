#Home {
  background-color: #f9f9f9;
}
#Home .search {
  height: 80rpx;
  background-image: url(data:image/jpeg;base64,);
  padding: 25rpx;
  padding-bottom: 0;
}
#Home .search .uni-flex .user {
  width: 200rpx;
  background-color: #69CFFD;
  border-radius: 45rpx;
  line-height: 0;
}
#Home .search .uni-flex .user uni-image {
  width: 65rpx;
  height: 65rpx;
  margin: 2rpx;
  margin-right: 10rpx;
  border-radius: 50%;
  vertical-align: middle;
}
#Home .search .uni-flex .user .text {
  font-size: 25rpx;
  line-height: 55rpx;
  margin-right: 10rpx;
  vertical-align: middle;
  color: #FFFFFF;
}
#Home .search .uni-flex .user .foter {
  font-size: 30rpx;
  vertical-align: middle;
  color: #FFFFFF;
}
#Home .search .uni-flex .imput {
  -webkit-flex: 1;
  -webkit-box-flex: 1;
          flex: 1;
  margin-left: 50rpx;
}
#Home .search .uni-flex .imput .uni-input {
  padding: 10rpx;
  padding-left: 35rpx;
  border-radius: 50rpx;
  background-color: #69CFFD;
  color: #FFFFFF;
  font-size: 25rpx;
}
#Home .search .uni-flex .imput .flip {
  position: absolute;
  top: 15rpx;
  right: 50rpx;
  color: #FFFFFF;
  font-size: 50rpx;
}
#Home .CourseEntrance {
  background-image: url(data:image/jpeg;base64,);
  height: 150rpx;
  position: relative;
}
#Home .ActivityCarousel {
  margin-top: 320rpx;
}
#Home .learning {
  background-color: #FFFFFF;
}
#Home .learning .CourseEntrance-box-Title {
  padding: 15rpx 45rpx;
}
#Home .learning .learningLi {
  padding-left: 25rpx;
  padding-right: 25rpx;
}
#Home .learning .learningLi .TabButton {
  padding-top: 25rpx;
}
#Home .ProjectActivities {
  margin-top: 35rpx;
  background-color: #FFFFFF;
  padding-bottom: 155rpx;
